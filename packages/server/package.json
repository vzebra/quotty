{
  "name": "@quotty/back",
  "version": "1.0.0",
  "description": "Random quote machine",
  "main": "./src/index.js",
  "repository": "https://git.epam.com/yevhen_horbunkov/quotty.git",
  "author": "Yevhen Horbunkov",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:server": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "start:server": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "dev": "yarn dev:server",
    "start": "yarn start:server",
    "test:unit": "yarn jest",
    "format:write": "yarn run prettier --write --ignore-unknown \"**/*\"",
    "format:check": "yarn run prettier --check --ignore-unknown \"**/*\"",
    "eslint:check": "yarn eslint \"src/**\"",
    "eslint:fix": "yarn eslint --fix \"src/**\"",
    "ts:check": "tsc --noEmit --project ./tsconfig.json",
    "clean": "rm -rf ./dist/*"
  },
  "devDependencies": {
    "@quotty/common": "1.0.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.3",
    "ttypescript": "^1.5.12",
    "typescript-transform-paths": "^3.0.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/joi": "^17.2.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/pg": "^7.14.11",
    "@types/sequelize": "^4.28.9",
    "@types/uuid": "^8.3.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.2"
  }
}
